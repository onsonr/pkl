@go.Package { name = "github.com/onsonr/pkljar/motrconfig" }
module MotrConfig

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.8.0#/go.pkl"

class AppConfig {
  domain: String 
}

class BitcoinConfig {
  chainId: String 
  valApiUri: String
  valRpcUri: String?
  valGrpcUri: String?
}

class CloudflareConfig {
  assetsBucket: String?
  globalDatabase: String?
}

class EthereumConfig {
  chainId: String 
  valApiUri: String
  valRpcUri: String?
  valGrpcUri: String?
}

class IpfsConfig {
  resolverUri: String
  apiUri: String?
}

class SonrConfig {
  chainId: String 
  valApiUri: String
  valRpcUri: String?
  valGrpcUri: String?
}

app : AppConfig
bitcoin: BitcoinConfig?
cloudflare: CloudflareConfig?
ethereum: EthereumConfig?
ipfs: IpfsConfig?
sonr: SonrConfig?
